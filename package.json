{
  "name": "@nestagencyuk/react_components",
  "version": "1.1.10",
  "description": "A collection of React pure components, built with TypeScript",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "dev": "start-storybook --port 3002 | webpack-dev-server",
    "icon:files": "ts-node --files ./index.ts iconFiles",
    "icon:stories": "ts-node --files ./index.ts iconStories",
    "scaffold": "ts-node --files ./index.ts scaffold --name=$NAME",
    "scaffold:stateful": "ts-node --files ./index.ts scaffold --name=$NAME --type=stateful",
    "test": "jest",
    "test:cypress": "cypress open",
    "test:unit": "jest --watch --verbose",
    "test:coverage": "npm run test && codecov --disable=gcov",
    "build": "npm run build:components && npm run build:docs",
    "build:components": "rm -rf ./dist && webpack --env.NODE_ENV=production",
    "build:docs": "build-storybook --docs --output-dir ./docs",
    "deploy": "git push && git push --tags",
    "clean": "rm ./package-lock.json && rm -rf ./node_modules && npm i"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nestagencyuk/react_components.git"
  },
  "keywords": [
    "react",
    "typescript"
  ],
  "author": "Milly Billins @ Nest Agency Ltd",
  "license": "ISC",
  "homepage": "https://github.com/nestagencyuk/react_components#readme",
  "dependencies": {
    "@nestagencyuk/scss_lib": "^1.1.2",
    "@nestagencyuk/typescript_lib-frontend": "^1.0.9",
    "@types/react-datepicker": "^3.0.2",
    "classnames": "^2.2.6",
    "colorthief": "^2.3.2",
    "react": "^16.13.1",
    "react-datepicker": "^3.1.3",
    "react-dom": "^16.13.1",
    "react-router": "^5.1.0",
    "react-router-dom": "^5.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.10.4",
    "@nestagencyuk/typescript_lib-backend": "^1.0.10",
    "@storybook/addon-a11y": "^5.3.19",
    "@storybook/addon-backgrounds": "^5.3.19",
    "@storybook/addon-docs": "^5.3.19",
    "@storybook/addon-viewport": "^5.3.19",
    "@storybook/preset-typescript": "^3.0.0",
    "@storybook/react": "^5.3.19",
    "@testing-library/dom": "^7.21.1",
    "@testing-library/jest-dom": "^5.11.1",
    "@testing-library/react": "^10.4.7",
    "@testing-library/react-hooks": "^3.3.0",
    "@types/classnames": "^2.2.9",
    "@types/enzyme": "^3.10.3",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/fs-extra": "^8.0.0",
    "@types/jest": "^26.0.5",
    "@types/node": "^12.7.4",
    "@types/react": "^16.9.2",
    "@types/react-dom": "^16.9.0",
    "@types/react-router-dom": "^5.1.0",
    "@types/webpack": "^4.39.1",
    "@typescript-eslint/eslint-plugin": "^2.27.0",
    "autoprefixer": "^9.6.1",
    "babel-loader": "^8.1.0",
    "babel-preset-react-app": "^9.1.1",
    "brotli-webpack-plugin": "^1.1.0",
    "codecov": "^3.6.5",
    "compression-webpack-plugin": "^3.1.0",
    "copy-webpack-plugin": "^5.0.4",
    "coveralls": "^3.0.9",
    "css-loader": "^3.2.0",
    "cssnano": "^4.1.10",
    "cypress": "^4.4.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "eslint": "^6.8.0",
    "eslint-config-standard-with-typescript": "^15.0.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-standard": "^4.0.1",
    "fork-ts-checker-webpack-plugin": "^4.1.6",
    "fs-extra": "^8.1.0",
    "ignore-loader": "^0.1.2",
    "jest": "^26.0.1",
    "make-runnable": "^1.3.6",
    "mini-css-extract-plugin": "^0.8.0",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.18.2",
    "prettier-webpack-plugin": "^1.2.0",
    "react-docgen-typescript-loader": "^3.3.0",
    "sass": "^1.25.0",
    "sass-loader": "^7.2.0",
    "standard": "^14.3.3",
    "svg-inline-loader": "^0.8.2",
    "ts-jest": "^26.1.0",
    "ts-loader": "^6.2.2",
